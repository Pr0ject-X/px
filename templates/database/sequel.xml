<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>ContentFilters</key>
        <dict/>
        <key>auto_connect</key>
        <true/>
        <key>data</key>
        <dict>
            <key>connection</key>
            <dict>
                <key>name</key>
                <string>{label}</string>
                <key>type</key>
                <string>SPTCPIPConnection</string>
                <key>database</key>
                <string>{database}</string>
                <key>host</key>
                <string>{host}</string>
                <key>user</key>
                <string>{username}</string>
                <key>password</key>
                <string>{password}</string>
                <key>port</key>
                <integer>{port}</integer>
                <key>rdbms_type</key>
                <string>{type}</string>
            </dict>
            <key>session</key>
            <dict/>
        </dict>
        <key>encrypted</key>
        <false/>
        <key>format</key>
        <string>connection</string>
        <key>queryFavorites</key>
        <array/>
        <key>queryHistory</key>
        <array/>
        <key>rdbms_type</key>
        <string>{type}</string>
        <key>version</key>
        <integer>1</integer>
    </dict>
</plist>
