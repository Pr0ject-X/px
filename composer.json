{
    "name": "pr0ject-x/px",
    "description": "Project-X is a pluggable toolkit which can streamline redundant tasks without hassle.",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "bin": [
        "bin/px"
    ],
    "authors": [
        {
            "name": "Travis Tomka",
            "email": "underoathosx@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "consolidation/robo": "^1.4",
        "symfony/cache": "^3.0|^4.0",
        "symfony/config": "^3.0|^4.0",
        "symfony/finder": "^3.0|^4.0",
        "symfony/console": "^3.0|^4.0",
        "symfony/http-client": "^5.4",
        "knplabs/packagist-api": "^1.5",
        "danielstjules/stringy": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Pr0jectX\\Px\\": "./src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mikey179/vfsstream": "^1.6",
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Pr0jectX\\Px\\Tests\\": "./tests"
        }
    }
}
